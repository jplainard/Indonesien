{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NODE_VERSION": "20.x"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NODE_VERSION": "20.x"
    }
  },
  "functions": {
    "src/app/**/*.ts": {
      "maxDuration": 30
    },
    "src/app/**/route.ts": {
      "maxDuration": 30
    }
  }
}
